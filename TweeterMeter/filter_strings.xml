<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<filter name="web_characters">
    <string text="http://"></string>
    <string text="www."></string>
    <string text=".org"></string>
    <string text=".com"></string>
</filter>
<filter name="invalid_characters">
    <string text="\"></string>
    <string text=","></string>
</filter>
<filter name="punctuation">
    <string text="."></string>
    <string text=","></string>
    <string text=":"></string>
    <string text=";"></string>
    <string text="'"></string>
    <string text='"'></string>
</filter>
<filter name="twitter_reserved_strings">
    <string text="rt"></string>
</filter>
</plist>